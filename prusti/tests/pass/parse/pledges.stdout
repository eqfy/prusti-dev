    g: u32,
}

fn borrows(_x: &mut u32) {
    let mut a = T{f: 1, g: 2,};
    if !(a.f == 1) {




        { ::std::rt::begin_panic("assertion failed: a.f == 1") }
    };
    if !(a.g == 2) {
        { ::std::rt::begin_panic("assertion failed: a.g == 2") }
    };
    let x = &mut a;
    let y = &mut x.f;
    let z = &x.g;
    *y = 5;
    if !(*z == 2) { { ::std::rt::begin_panic("assertion failed: *z == 2") } };
    if !(a.f == 5) {
        { ::std::rt::begin_panic("assertion failed: a.f == 5") }
    };
}
